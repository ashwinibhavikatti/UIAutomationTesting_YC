<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
	<parameter name="url" value="http://localhost:3000/" />
	<parameter name="browserName" value="chrome" />
	<listeners>
		<listener class-name="com.assignment.test.TestListener"></listener>
	</listeners>
	<test thread-count="5" name="TestOnlyNewRegistration">
		<groups>
			<run>
				<include name="edit" />
			</run>
		</groups>
		<classes>
			<class name="com.assignment.test.NewRegistration"></class>
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
