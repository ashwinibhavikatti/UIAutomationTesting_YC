	<test thread-count="5" name="TestNewRegistration">
		<parameter name="browserName" value="edge" />
		<classes>
			<class name="com.assignment.test.NewRegistration" />
		</classes>
	</test> <!-- Test -->
	<test thread-count="5" name="TestFieldValidation">
		<parameter name="browserName" value="firefox" />
